<div>
    <h1>Products</h1>
    <div>
        {{#each products}}
            <div class="products">
                <p>Name: <strong>{{this.title}}</strong></p>
                <p>Price: {{this.price}}</p>
                <p>Description: {{this.description}}</p>
                <p>Stock: {{this.stock}}</p>
                <p>Category: {{this.category}}</p>
            </div>
        {{/each}}
    </div>
</div>
<script src="/socket.io/socket.io.js"></script>